  "description": "iDurianMedia Universal Scaffold",
  "main": "",
  "engines": {
    "node": ">=0.12.0"
  },
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "redux",
    "react-router",
    "boilerplate",
    "scaffold",
