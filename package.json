    "eslint": "^1.10.3",
    "eslint-config-airbnb": "3.0.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
