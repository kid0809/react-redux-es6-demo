  "repository": {
    "type": "git",
    "url": "https://github.com/timnity/iDurianMedia-Universal-Scaffold.git"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "testwatch": "npm run test -- --watch",
    "startweb": "nodemon --harmony babel.webserver.js -w ./web/bundle",
    "startapi": "nodemon --harmony babel.apiserver.js -w ./api",
    "builddev": "webpack -d --verbose --display-error-details --progress --config ./config/webdev.webpack.cfg.js",
    "buildprod": "NODE_ENV=production webpack -p --verbose --display-error-details --progress --config ./config/webprod.webpack.cfg.js"
  },
  "dependencies": {
